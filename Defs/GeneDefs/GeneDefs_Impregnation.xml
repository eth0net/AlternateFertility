    <iconPath>UI/Icons/Genes/Gene_Androdite</iconPath>
    <displayOrderInCategory>210</displayOrderInCategory>
  </GeneDef>

  <GeneDef ParentName="ImpregnationBase">
    <defName>Hermaphrodite</defName>
    <label>hermaphrodite</label>
